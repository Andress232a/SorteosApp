{
  "expo": {
    "name": "PremioClick",
    "slug": "premioclick",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#7b2cbf"
    },
    "icon": "./assets/logo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.premioclick.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#7b2cbf"
      },
      "package": "com.premioclick.app",
      "usesCleartextTraffic": true
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "premioclick",
    "schemes": ["premioclick", "sorteosapp"]
  }
}

